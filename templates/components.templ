					</div>
				</div>

				<div>
					<div class="contact-form">
						<h3 class="subsection-title component-margin-medium">Get Started Today</h3>
						
						<!-- Success/Error Messages -->
						<div id="form-message" class="hidden form-message" role="alert" aria-live="polite"></div>
						
						<form id="contact-form" class="space-y-6" novalidate>
							<div class="form-group">
								<label for="name" class="form-label">
									Name
									<span class="required-asterisk" aria-label="required">*</span>
								</label>
								<input 
									type="text" 
									id="name" 
									name="name" 
									required 
									minlength="2" 
									maxlength="100" 
									class="form-input" 
									placeholder="Your Name"
									aria-describedby="name-error name-help"
									autocomplete="name"
								/>
								<div class="field-description" id="name-help">Enter your full name (minimum 2 characters)</div>
								<div id="name-error" class="field-error hidden" role="alert"></div>
							</div>
							
							<div class="form-group">
								<label for="company" class="form-label">Company</label>
								<input 
									type="text" 
									id="company" 
									name="company" 
									maxlength="100" 
									class="form-input" 
									placeholder="Your Company"
									aria-describedby="company-help"
									autocomplete="organization"
								/>
								<div class="field-description" id="company-help">Optional: Your organization or company name</div>
							</div>
							
							<div class="form-group">
								<label for="email" class="form-label">
									Email Address
									<span class="required-asterisk" aria-label="required">*</span>
								</label>
								<input 
									type="email" 
									id="email" 
									name="email" 
									required 
									class="form-input" 
									placeholder="your@email.com"
									aria-describedby="email-error email-help"
									autocomplete="email"
								/>
								<div class="field-description" id="email-help">We'll use this to contact you about your project</div>
								<div id="email-error" class="field-error hidden" role="alert"></div>
							</div>
							
							<div class="form-group">
								<label for="project" class="form-label">
									Project Description
									<span class="required-asterisk" aria-label="required">*</span>
								</label>
								<textarea 
									id="project" 
									name="project" 
									rows="5" 
									required 
									minlength="10" 
									maxlength="2000" 
									class="form-input form-textarea" 
									placeholder="Tell me about your digital asset project..."
									aria-describedby="project-error project-help project-count"
								></textarea>
								<div class="flex justify-between items-center mt-2">
									<div class="field-description" id="project-help">Describe your project needs, goals, and timeline (minimum 10 characters)</div>
									<div class="field-description" id="project-count">0 / 2000</div>
								</div>
								<div id="project-error" class="field-error hidden" role="alert"></div>
							</div>
							
							<button 
								type="submit" 
								id="submit-btn" 
								class="btn-primary w-full py-4 disabled:opacity-50 disabled:cursor-not-allowed"
								aria-describedby="submit-help"
							>
								<span id="submit-text">Start Your Project</span>
								<span id="submit-loading" class="hidden">
									<svg class="btn-spinner" viewBox="0 0 24 24" fill="none">
										<circle class="opacity-25" cx="12" cy="12" r="10" stroke="currentColor" stroke-width="4"></circle>
										<path class="opacity-75" fill="currentColor" d="m4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z"></path>
									</svg>
									Sending Message...
								</span>
							</button>
							<div class="field-description text-center" id="submit-help">Your message will be sent securely and we'll respond within 24 hours</div>
						</form>
						
						<script>
							// Professional Contact Form with Validation and Accessibility
							(function() {
								const form = document.getElementById('contact-form');
								const submitBtn = document.getElementById('submit-btn');
								const submitText = document.getElementById('submit-text');
								const submitLoading = document.getElementById('submit-loading');
								const messageDiv = document.getElementById('form-message');
								const projectTextarea = document.getElementById('project');
								const projectCount = document.getElementById('project-count');
								
								// Form validation rules
								const validationRules = {
									name: {
										required: true,
										minLength: 2,
										maxLength: 100,
										pattern: /^[a-zA-Z\s\u00C0-\u024F\u1E00-\u1EFF]+$/,
										message: 'Please enter a valid name (letters and spaces only, 2-100 characters)'
									},
									email: {
										required: true,
										pattern: /^[^\s@]+@[^\s@]+\.[^\s@]+$/,
										message: 'Please enter a valid email address'
									},
									project: {
										required: true,
										minLength: 10,
										maxLength: 2000,
										message: 'Please describe your project (10-2000 characters)'
									}
								};
								
								// Real-time validation function
								function validateField(field) {
									const fieldName = field.name;
									const value = field.value.trim();
									const rules = validationRules[fieldName];
									const errorDiv = document.getElementById(`${fieldName}-error`);
									
									if (!rules) return true;
									
									let isValid = true;
									let errorMessage = '';
									
									// Required field check
									if (rules.required && !value) {
										isValid = false;
										errorMessage = `${fieldName.charAt(0).toUpperCase() + fieldName.slice(1)} is required`;
									}
									// Length validation
									else if (value && rules.minLength && value.length < rules.minLength) {
										isValid = false;
										errorMessage = `Minimum ${rules.minLength} characters required`;
									}
									else if (value && rules.maxLength && value.length > rules.maxLength) {
										isValid = false;
										errorMessage = `Maximum ${rules.maxLength} characters allowed`;
									}
									// Pattern validation
									else if (value && rules.pattern && !rules.pattern.test(value)) {
										isValid = false;
										errorMessage = rules.message;
									}
									
									// Update UI
									if (isValid) {
										field.classList.remove('error');
										field.classList.add('success');
										errorDiv.classList.add('hidden');
										errorDiv.innerHTML = '';
									} else {
										field.classList.remove('success');
										field.classList.add('error');
										errorDiv.classList.remove('hidden');
										errorDiv.innerHTML = `
											<svg class="w-4 h-4 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
												<path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7 4a1 1 0 11-2 0 1 1 0 012 0zm-1-9a1 1 0 00-1 1v4a1 1 0 102 0V6a1 1 0 00-1-1z" clip-rule="evenodd"/>
											</svg>
											${errorMessage}
										`;
									}
									
									return isValid;
								}
								
								// Character counter for project description
								function updateCharacterCount() {
									const currentLength = projectTextarea.value.length;
									projectCount.textContent = `${currentLength} / 2000`;
									
									if (currentLength > 1800) {
										projectCount.style.color = 'var(--accent-red)';
									} else if (currentLength > 1500) {
										projectCount.style.color = 'var(--text-muted)';
									} else {
										projectCount.style.color = 'var(--text-muted)';
									}
								}
								
								// Show success message
								function showMessage(message, type) {
									messageDiv.classList.remove('hidden');
									messageDiv.className = `form-message ${type}`;
									
									if (type === 'success') {
										messageDiv.innerHTML = `
											<svg class="w-5 h-5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
												<path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/>
											</svg>
											${message}
										`;
									} else {
										messageDiv.innerHTML = `
											<svg class="w-5 h-5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
												<path fill-rule="evenodd" d="M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7 4a1 1 0 11-2 0 1 1 0 012 0zm-1-9a1 1 0 00-1 1v4a1 1 0 102 0V6a1 1 0 00-1-1z" clip-rule="evenodd"/>
											</svg>
											${message}
										`;
									}
									
									// Scroll to message
									messageDiv.scrollIntoView({ behavior: 'smooth', block: 'center' });
									messageDiv.focus();
								}
								
								// Form submission
								form.addEventListener('submit', async function(e) {
									e.preventDefault();
									
									// Clear previous messages
									messageDiv.classList.add('hidden');
									
									// Validate all fields
									const nameValid = validateField(document.getElementById('name'));
									const emailValid = validateField(document.getElementById('email'));
									const projectValid = validateField(document.getElementById('project'));
									
									if (!nameValid || !emailValid || !projectValid) {
										showMessage('Please correct the errors above before submitting.', 'error');
										// Focus first invalid field
										const firstErrorField = form.querySelector('.error');
										if (firstErrorField) {
											firstErrorField.focus();
										}
										return;
									}
									
									// Show loading state
									submitBtn.disabled = true;
									submitBtn.setAttribute('aria-busy', 'true');
									submitText.classList.add('hidden');
									submitLoading.classList.remove('hidden');
									
									try {
										const formData = new FormData(form);
										const response = await fetch('/contact', {
											method: 'POST',
											body: formData
										});
										
										const result = await response.json();
										
										if (result.success) {
											showMessage('Thank you! Your message has been sent successfully. We\'ll get back to you within 24 hours.', 'success');
											form.reset();
											// Clear validation states
											form.querySelectorAll('.form-input').forEach(input => {
												input.classList.remove('success', 'error');
											});
											updateCharacterCount();
										} else {
											showMessage(result.message || 'Sorry, there was an error sending your message. Please try again.', 'error');
										}
									} catch (error) {
										console.error('Form submission error:', error);
										showMessage('Sorry, there was a network error. Please check your connection and try again.', 'error');
									} finally {
										// Reset button state
										submitBtn.disabled = false;
										submitBtn.setAttribute('aria-busy', 'false');
										submitText.classList.remove('hidden');
										submitLoading.classList.add('hidden');
									}
								});
								
								// Real-time validation event listeners
								['name', 'email', 'project'].forEach(fieldName => {
									const field = document.getElementById(fieldName);
									field.addEventListener('blur', () => validateField(field));
									field.addEventListener('input', () => {
										// Clear error state on input
										if (field.classList.contains('error')) {
											field.classList.remove('error');
											const errorDiv = document.getElementById(`${fieldName}-error`);
											errorDiv.classList.add('hidden');
										}
									});
								});
								
								// Character counter
								projectTextarea.addEventListener('input', updateCharacterCount);
								updateCharacterCount(); // Initialize counter
								
								// Keyboard navigation enhancements
								form.addEventListener('keydown', function(e) {
									// Submit form with Ctrl/Cmd + Enter
									if ((e.ctrlKey || e.metaKey) && e.key === 'Enter') {
										e.preventDefault();
										form.requestSubmit();
									}
								});
								
								// Enhanced focus management
								form.addEventListener('focusin', function(e) {
									if (e.target.matches('.form-input')) {
										e.target.closest('.form-group').classList.add('focused');
									}
								});
								
								form.addEventListener('focusout', function(e) {
									if (e.target.matches('.form-input')) {
										e.target.closest('.form-group').classList.remove('focused');
									}
								});
								
							})();
						</script>

						<div class="mt-8 pt-8 border-t border-default">
							<p class="text-center text-secondary mb-4">Prefer to reach out directly?</p>
							<div class="flex justify-center space-x-6">
								<a href="mailto:hello@holger-hahn.net" class="text-primary hover:text-primary flex items-center space-x-2">
									<svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 8l7.89 4.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"/>
									</svg>
									<span>Email</span>
								</a>
								<a href="https://www.linkedin.com/in/holger-hahn-1b258ba3/" class="text-primary hover:text-primary flex items-center space-x-2">
									<svg class="w-5 h-5" fill="currentColor" viewBox="0 0 20 20">
										<path fill-rule="evenodd" d="M16.338 16.338H13.67V12.16c0-.995-.017-2.277-1.387-2.277-1.39 0-1.601 1.086-1.601 2.207v4.248H8.014v-8.59h2.559v1.174h.037c.356-.675 1.227-1.387 2.526-1.387 2.703 0 3.203 1.778 3.203 4.092v4.711zM5.005 6.575a1.548 1.548 0 11-.003-3.096 1.548 1.548 0 01.003 3.096zm-1.337 9.763H6.34v-8.59H3.667v8.59zM17.668 1H2.328C1.595 1 1 1.581 1 2.328v15.403C1 18.418 1.595 19 2.328 19h15.34c.734 0 1.332-.582 1.332-1.299V2.328C19 1.581 18.402 1 17.668 1z" clip-rule="evenodd"/>
									</svg>
									<span>LinkedIn</span>
								</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
}

// MetricProgressBar displays a visual progress bar for metrics
templ MetricProgressBar(label string, before, after float64, unit string) {
	<div class="metric-progress-bar mb-4">
		<div class="flex justify-between items-center mb-2">
			<span class="text-sm font-medium text-gray-700">{ label }</span>
			<div class="text-right">
				<span class="text-lg font-bold text-green-600">{ fmt.Sprintf("%.1f%s", after, unit) }</span>
				<span class="text-sm text-gray-500 ml-2">({ fmt.Sprintf("%.1f%s", before, unit) } → { fmt.Sprintf("%.1f%s", after, unit) })</span>
			</div>
		</div>
		<div class="relative">
			<div class="bg-gray-200 rounded-full h-3">
				<div 
					class="bg-gradient-to-r from-blue-500 to-green-500 h-3 rounded-full transition-all duration-1000 ease-out"
					style={ fmt.Sprintf("width: %.1f%%", (after/100)*100) }
				></div>
			</div>
			<div class="flex justify-between text-xs text-gray-500 mt-1">
				<span>0{ unit }</span>
				<span>100{ unit }</span>
			</div>
		</div>
		<div class="text-sm text-green-600 font-medium mt-2">
			↗ +{ fmt.Sprintf("%.1f", after-before) }{ unit } improvement
		</div>
	</div>
}

// TimeMetricCard displays time-based improvements
templ TimeMetricCard(title string, beforeMinutes, afterMinutes int, unit string) {
	<div class="bg-white p-6 rounded-lg border border-gray-200 shadow-sm">
		<h4 class="font-semibold text-gray-900 mb-3">{ title }</h4>
		<div class="grid grid-cols-2 gap-4">
			<div class="bg-red-50 p-4 rounded-lg border border-red-200">
				<div class="text-sm text-red-600 font-medium mb-1">Before</div>
				<div class="text-2xl font-bold text-red-700">
					if unit == "minutes" && beforeMinutes >= 60 {
						{ fmt.Sprintf("%.1fh", float64(beforeMinutes)/60) }
					} else {
						{ fmt.Sprintf("%d%s", beforeMinutes, unit) }
					}
				</div>
			</div>
			<div class="bg-green-50 p-4 rounded-lg border border-green-200">
				<div class="text-sm text-green-600 font-medium mb-1">After</div>
				<div class="text-2xl font-bold text-green-700">
					if unit == "minutes" && afterMinutes >= 60 {
						{ fmt.Sprintf("%.1fh", float64(afterMinutes)/60) }
					} else {
						{ fmt.Sprintf("%d%s", afterMinutes, unit) }
					}
				</div>
			</div>
		</div>
		<div class="mt-4 p-3 bg-blue-50 rounded-lg border border-blue-200">
			<div class="text-sm text-blue-600 font-medium">Improvement</div>
			<div class="text-xl font-bold text-blue-700">
				{ fmt.Sprintf("%.0f%% faster", float64(beforeMinutes-afterMinutes)/float64(beforeMinutes)*100) }
			</div>
		</div>
	</div>
}

