			if metrics.TestCoverage != nil {
				<div class="col-span-full">
					@MetricProgressBar("Test Coverage", metrics.TestCoverage.Before, metrics.TestCoverage.After, metrics.TestCoverage.Unit)
				</div>
			}
			
			if metrics.DeploymentTime != nil {
				@TimeMetricCard("Deployment Time", metrics.DeploymentTime.Before, metrics.DeploymentTime.After, metrics.DeploymentTime.Unit)
			}
			
			if metrics.SystemReliability != nil {
				@SystemReliabilityCard(metrics.SystemReliability.Uptime, metrics.SystemReliability.MTBF, metrics.SystemReliability.MTTR, metrics.SystemReliability.Incidents)
			}
			
			if metrics.Productivity != nil {
